;
;	objcolor,rgbcolor命令テスト
;
syscolor 15:boxf

sdim buf,$10000
buf="テキスト入力ボックスに任意の色を指定できます"
objcolor 0,0,240
color 128,255,128
objmode objmode_usecolor+objmode_guifont
mesbox buf,320,200,1+8
color 0,0,0
mes "[ctrl]+[A]で全選択します"

pos 0,240
a=123:b=456
objcolor 0,255,0

color 255,128,128
input a
color 128,128,255
input b

rgbcolor $ff0000
mes "RED"
rgbcolor $00ff00
mes "GREEN"
rgbcolor $0000ff
mes "BLUE"

stop



