ONIPAD MENU 
BEGIN
    POPUP "ファイル(&F)"
    BEGIN
        MENUITEM "新規作成(&N)\tctrl+N",            IDM_NEW
        MENUITEM "開く(&O)...\tctrl+O",           IDM_OPEN
        MENUITEM "上書き保存(&S)\tctrl+S",           IDM_SAVE
        MENUITEM "名前を付けて保存(&A)...",             IDM_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "印刷(&P)...\tctrl+P",           IDM_PRINT
        MENUITEM SEPARATOR
        MENUITEM "終了(&Q)\tctrl+Q",              IDM_EXIT
    END
    POPUP "編集(&E)"
    BEGIN
        MENUITEM "やり直し(&U)\tctrl+Z",            IDM_UNDO
        MENUITEM SEPARATOR
        MENUITEM "切り取り(&T)\tctrl+X",            IDM_CUT
        MENUITEM "コピー(&C)\tctrl+C",             IDM_COPY
        MENUITEM "貼り付け(&P)\tctrl+V",            IDM_PASTE
        MENUITEM "削除(&D)\tDelete",              IDM_CLEAR
        MENUITEM SEPARATOR
        MENUITEM "すべてを選択(&A)\tctrl+A",          IDM_SELALL
    END
    POPUP "カーソル(&L)"
    BEGIN
        MENUITEM "先頭行に移動(&T)\tctrl+T",          IDM_LTOP
        MENUITEM "最終行に移動(&B)\tctrl+B",          IDM_LBTM
        MENUITEM "指定行に移動(&J)...\tctrl+J",       IDM_JUMP
        MENUITEM "ラベル一覧...(&L)\tF11",           IDM_LABEL
    END
    POPUP "検索(&S)"
    BEGIN
        MENUITEM "文字列の検索(&F)...\tF4",           IDM_FIND
        MENUITEM "次を検索(&N)\tF3",                IDM_NEXT
        MENUITEM "文字列の置換(&R)...",               IDM_REPLACE
    END
    POPUP "オプション(&O)"
    BEGIN
        MENUITEM "フォント(&F)...",                 IDM_FONT
        MENUITEM "背景カラー(&B)",                   IDM_BGCOLOR
        MENUITEM "ツールバー(&T)",                   IDM_VIEW_TOOLBAR
        MENUITEM "ステータスバー(&S)",                 IDM_VIEW_STATUS
        MENUITEM "スクロールバー(&H)",                 IDM_HSCROLL
        MENUITEM "起動時ディレクトリ(&D)...",            IDM_STARTDIR
    END
    POPUP "ＨＳＰ(&P)"
    BEGIN
        MENUITEM "コンパイル+実行(&C)\tF5",            IDM_COMP
        MENUITEM "実行(&R)\tF6",                  IDM_RUN
        MENUITEM "コンパイルのみ(&P)\tF7",             IDM_COMP2
        MENUITEM "エラー表示(&E)...\tF8",            IDM_HSPERR
        MENUITEM "オブジェクトファイル作成(&B)\tF9",        IDM_MKOBJ
        MENUITEM "START.AXファイル作成(&S)",          IDM_MKOBJ2
        MENUITEM "実行ファイル自動作成(&A)\tctrl+F9",     IDM_AUTOMAKE
        MENUITEM "外部ファイル実行(&F)...\tF10",        IDM_COMP3
        MENUITEM "予約キーワード一覧(&K)...",            IDM_HSPSYM
        MENUITEM SEPARATOR
        MENUITEM "起動オプション(&O)...",              IDM_CMDOPT
        MENUITEM "Debugウインドゥ表示(&D)",            IDM_DEBUG
    END
    POPUP "ツール(&T)"
    BEGIN
        MENUITEM "PACKFILE編集(&E)...",           IDM_PACKED
        MENUITEM "DPMファイル作成(&P)",               IDM_PACKGO
        MENUITEM "EXEファイル作成(&M)",               IDM_MKEXE1
        MENUITEM "スクリーンセーバー作成(&S)",             IDM_MKEXE2
        MENUITEM "ソースフォルダを開く(&O)...",           IDM_OPEN_SRCFOLDER
        MENUITEM "HSPアシスタント起動(&F)...",          IDM_START_RUNTIMEMAN
    END
    POPUP "ヘルプ(&H)"
    BEGIN
        MENUITEM "HSPキーワード検索(&K)...\tF1",       IDM_KWHELP
        MENUITEM "HSPヘルプ設定...(&D)",             IDM_HELPDIR
        MENUITEM "HSPプログラムガイドを開く(&P)...",       IDM_HSPMAN1
        MENUITEM "HSP命令リファレンスを開く(&R)...",       IDM_HSPMAN2
        MENUITEM "HSPマニュアル目次(&I)...",           IDM_HSPMAN3
        MENUITEM "バージョン情報(&A)...",              IDM_ABOUT
    END
END


